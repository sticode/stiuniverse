<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="assettools" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/assettools" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DDEBUG" />
				</Compiler>
				<Linker>
					<Add directory="../StiGame/bin/Debug" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c python ../copy_bin.py debug" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/assettools" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/Release/" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DRELEASE" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../StiGame/bin/Release" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c python ../copy_bin.py release" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="../include/SDL2" />
			<Add directory="../StiGame" />
		</Compiler>
		<Linker>
			<Add option="-lSDL2main" />
			<Add option="-lSDL2" />
			<Add option="-lSDL2_image" />
			<Add option="-lSDL2_ttf" />
			<Add option="-lSDL2_gfx" />
			<Add option="-lStiGame" />
			<Add directory="../lib" />
		</Linker>
		<Unit filename="AssetGenerator.cpp" />
		<Unit filename="AssetGenerator.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
